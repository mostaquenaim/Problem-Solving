#include <iostream>

using namespace std;

int main()
{
    int t;
    cin>>t;
    
    while(t--){
        int n,a;
        cin>>n;
        
        bool flag=false;
        
        for(int i=1;i<=n;i++){
            cin>>a;
            if(a>i) continue; 
            else flag=true;
        }
        
        if(flag){
            cout<<"YES";
            
        }
        else cout<<"NO";
        cout<<endl;
    }
}
